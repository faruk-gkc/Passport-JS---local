<div class="card">
    <div class="card-header">Kayıtlı Kullanıcılar</div>

        <div class="card-body">

            <ul class="list-group">
                {{#each users}}
                    <li class="list-group-item">{{username}}</li>
                {{else}}
                    <div class="alert alert-danger">

                        Kayıtlı Kullanıcı Yok..
                    </div>
                {{/each}} 
            </ul>
    </div>

    {{#if user}}
        <h5>{{user._id}}</h5>
    {{/if}}

</div>